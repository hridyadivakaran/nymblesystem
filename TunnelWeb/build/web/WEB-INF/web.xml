<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>TunnelServlet</servlet-name>
        <servlet-class>com.topk.client.tunnel.TunnelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObjectTunnelServlet</servlet-name>
        <servlet-class>com.topk.client.tunnel.ObjectTunnelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TunnelServlet</servlet-name>
        <url-pattern>/TunnelServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObjectTunnelServlet</servlet-name>
        <url-pattern>/ObjectTunnelServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
